<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/LinearLayout01"
    android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:gravity="center" android:layout_width="200dip">
    
<TableRow android:id="@+id/TableRow02" android:layout_width="wrap_content" android:layout_height="wrap_content"><TextView android:id="@+id/TextView07" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Lat">
</TextView><EditText android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="0" android:id="@+id/Lat" android:inputType="numberDecimal">
</EditText></TableRow><TableRow android:id="@+id/TableRow02" android:layout_width="wrap_content" android:layout_height="wrap_content"><TextView android:id="@+id/TextView07" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Lon">
</TextView><EditText android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="0" android:id="@+id/Lon" android:inputType="numberDecimal">
</EditText></TableRow><TableRow android:id="@+id/TableRow07" android:layout_width="wrap_content" android:layout_height="wrap_content">
<TextView android:id="@+id/TextView07" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="RA">
</TextView>
<EditText android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="0" android:id="@+id/RA" android:inputType="text">
</EditText>
</TableRow>

<TableRow android:id="@+id/TableRow07" android:layout_width="wrap_content" android:layout_height="wrap_content">
<TextView android:id="@+id/TextView07" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Dec">
</TextView>
<EditText android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="0" android:id="@+id/Dec" android:inputType="numberDecimal">
</EditText>

</TableRow>

<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content"><TextView android:id="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="AZ Offset (Degs)"></TextView>

<EditText android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/AzOffset" android:text="0" android:inputType="numberSigned"></EditText>
</TableRow><TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content"><Button android:id="@+id/Button01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Cancel"></Button>
<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="OK" android:id="@+id/OkButton"></Button>
</TableRow>

</LinearLayout>
